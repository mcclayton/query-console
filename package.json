{
  "name": "query-console",
  "version": "1.0.8",
  "description": "A console for tracking and inspecting the queries being made by applications via tailing and parsing log files.",
  "main": "src/index.js",
  "scripts": {
    "build": "parcel build src/client/index.html",
    "start:client": "serve dist/",
    "start:server": "node src/server/index.js",
    "start:client:dev": "parcel src/client/index.html --open",
    "start": "./bin/start.sh",
    "test": "test"
  },
  "author": "Michael Clayton",
  "preferGlobal": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prepublishOnly": "npm run build",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "node-sass": "^4.9.3",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.3",
    "recompose": "^0.28.2",
    "serve": "^10.0.0",
    "tail": "^1.3.0"
  },
  "devDependencies": {
    "babel-preset-preact": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2"
  },
  "bin": {
    "query-console": "./bin/start.sh"
  }
}
